<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.example.qb-port-sync</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/qb-port-sync</string>
        <string>--config</string>
        <string>/Library/Application Support/qb-port-sync/config.toml</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/var/log/qb-port-sync.log</string>

    <key>StandardErrorPath</key>
    <string>/var/log/qb-port-sync.log</string>

    <key>WorkingDirectory</key>
    <string>/usr/local/bin</string>
</dict>
</plist>
